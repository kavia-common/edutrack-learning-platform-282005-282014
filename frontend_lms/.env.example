# Frontend LMS environment variables (placeholders only - do not commit secrets)

# Base API endpoint for REST calls
REACT_APP_API_BASE=

# Backend URL (if different from API base)
REACT_APP_BACKEND_URL=

# Public frontend URL
REACT_APP_FRONTEND_URL=

# WebSocket URL
REACT_APP_WS_URL=

# Node environment for the React app
REACT_APP_NODE_ENV=development

# Disable Next.js telemetry (kept for compatibility if imported code referenced it)
REACT_APP_NEXT_TELEMETRY_DISABLED=true

# Generate source maps during build
REACT_APP_ENABLE_SOURCE_MAPS=false

# Port to run the dev server
REACT_APP_PORT=3000

# Trust proxy setting (stringified)
REACT_APP_TRUST_PROXY=false

# Log level (e.g., debug, info, warn, error)
REACT_APP_LOG_LEVEL=info

# Healthcheck path exposed by the frontend (if used)
REACT_APP_HEALTHCHECK_PATH=/healthz

# Feature flags (comma separated or JSON string as preferred by app)
REACT_APP_FEATURE_FLAGS=

# Experiments enabled flag
REACT_APP_EXPERIMENTS_ENABLED=false
